{
    "compilerOptions": {
      "target": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "typeRoots": ["node_modules/@types/"],
      "declaration": true,
      "declarationDir": "dts",
      "emitDeclarationOnly": true,
      "allowSyntheticDefaultImports": true,
    }
  }